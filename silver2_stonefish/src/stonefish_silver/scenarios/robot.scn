        <base_link name="Main_Body" type="model" physics="submerged">
			<physical>
				<mesh filename="Body.obj" scale="0.2"/>
				<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
				<thickness value="0.005"/>
			</physical>
			<visual>
				<mesh filename="Body.obj" scale="0.2"/>
				<origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
			</visual>
			<material name="$(arg body_material)"/>
			<look name="Silver"/>
			<world_transform xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
        </base_link>

		<!-- Leg 0 -->
		<!-- Coxa L0 -->
		<link name="Coxa_L0" type="model" physics="surface">
