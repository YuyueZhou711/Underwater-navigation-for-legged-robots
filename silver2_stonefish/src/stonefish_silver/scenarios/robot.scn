            <parent name="Main_Body"/>
            <child name="Coxa_L0"/>
            <origin xyz="0.324 0.196 0.056" rpy="0.0 0.0 0.0"/>
            <axis xyz="0.0 0.0 1.0"/>
        </joint>
		<actuator name="Servo_L0_Coxa" type="servo">
            <controller position_gain="$(arg servo_pos_gain)" velocity_gain="1.0" max_torque="$(arg servo_max_torque)" max_velocity="$(arg servo_max_vel)"/>
            <joint name="Joint_L0_Coxa"/>
            <initial position="0.75"/>
